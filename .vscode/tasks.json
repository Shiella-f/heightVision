{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "CMake Configure",
      "type": "process",
      "command": "${workspaceFolder}\\.vscode\\cmake_configure.bat",
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "panel": "shared"
      },
      "problemMatcher": []
    },
    {
      "label": "CMake Build (Debug)",
      "type": "process",
      "command": "${workspaceFolder}\\.vscode\\cmake_build.bat",
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "panel": "shared"
      },
      "problemMatcher": []
    },
    {
      "label": "CMake Build (Release)",
      "type": "process",
      "command": "${workspaceFolder}\\.vscode\\cmake_build_release.bat",
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "panel": "shared"
      },
      "problemMatcher": []
    }
  ]
}
